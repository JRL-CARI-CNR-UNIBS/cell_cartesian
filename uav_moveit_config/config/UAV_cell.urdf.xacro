<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro" name="UAV_cell">
    <xacro:arg name="initial_positions_file" default="initial_positions.yaml" />

    <!-- Import UAV_cell urdf file -->
    <xacro:include filename="$(find cartesian_description)/urdf/uav_cell.xacro" />

    <!-- Import control_xacro -->
    <xacro:include filename="UAV_cell.ros2_control.xacro" />


    <xacro:UAV_cell_ros2_control name="FakeSystem" initial_positions_file="$(arg initial_positions_file)"/>

</robot>
